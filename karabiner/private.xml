<?xml version="1.0"?>
<root>
    <item>
        <name>Leave GT</name>
        <identifier>private.leave_gt</identifier>
        <autogen>
            --KeyToKey--
            KeyCode::COMMA, ModifierFlag::SHIFT_L,
            KeyCode::COMMA, ModifierFlag::SHIFT_L
        </autogen>
    </item>
    <item>
        <name>Comma to Backspace</name>
        <identifier>private.comma_to_backspace</identifier>
        <autogen>
            --KeyToKey--
            KeyCode::COMMA,
            KeyCode::DELETE
        </autogen>
    </item>
    <item>
        <name>Backspace to Comma</name>
        <identifier>private.backspace_to_comma</identifier>
        <autogen>
            --KeyToKey--
            KeyCode::DELETE,
            KeyCode::COMMA
        </autogen>
    </item>
    <item>
        <name>Switch to Desktop 1</name>
        <identifier>private.desktop_1</identifier>
        <autogen>
            --KeyToKey--
            KeyCode::F1,
            KeyCode::1, ModifierFlag::CONTROL_L
        </autogen>
    </item>
    <item>
        <name>Switch to Desktop 2</name>
        <identifier>private.desktop_2</identifier>
        <autogen>
            --KeyToKey--
            KeyCode::F2,
            KeyCode::2, ModifierFlag::CONTROL_L
        </autogen>
    </item>
</root>
